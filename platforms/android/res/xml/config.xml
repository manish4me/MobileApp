<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <allow-intent href="market:*" />
    <icon src="res/android/appicon.png" />
    <splash density="land-hdpi" src="res/android/splash.png" />
    <splash density="land-ldpi" src="res/android/splash.png" />
    <splash density="land-mdpi" src="res/android/splash.png" />
    <splash density="land-xhdpi" src="res/android/splash.png" />
    <splash density="port-hdpi" src="res/android/splash.png" />
    <splash density="port-ldpi" src="res/android/splash.png" />
    <splash density="port-mdpi" src="res/android/splash.png" />
    <splash density="port-xhdpi" src="res/android/splash.png" />
    <name>Prescription Vault</name>
    <description>
       An useful app for all the medical needs at your fingertips.
    </description>
    <author email="manish4me@gmail.com" href="http://cordova.io">
       Manish, Arindam and Prescription Vault Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="SplashScreenDelay" value="10000" />
</widget>
